<% @title = 'Добавление нового вопроса' %>
<%= link_to '<< к списку', test_questions_path(@test), method: :get %>
<%= form_with model: @question, url: "/tests/#{params[:test_id]}/questions", method: :post do |form| %>
  <p>
    <%= form.label :body, "Текст" %>
    <%= form.text_field :body %>
  </p>
  <p><%= form.submit %></p>
<% end %>
